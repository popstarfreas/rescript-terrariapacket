{
  "name": "terraria-packet",
  "version": "1.2.0",
  "main": "dist/Export.js",
  "types": "dist/Export.d.ts",
  "exports": {
    ".": {
      "import": "./dist/Export.js",
      "require": "./dist/Export.js",
      "types": "./dist/Export.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "build:watch": "rescript watch",
    "build:bundle": "rsync -a --include='*/' --include='*.d.ts' --exclude='*' src/ lib/ && esbuild src/Export.ts --bundle --outfile=dist/Export.js --format=cjs --platform=node && tsc -p . && api-extractor run --local",
    "build": "rescript",
    "build:tsc": "tsc -p .",
    "test": "pta  'tests/*.test.js'"
  },
  "dependencies": {
    "@popstarfreas/packetfactory": "^6.3.0",
    "@rescript/core": "^1.6.1",
    "rescript": "^12.0.0"
  },
  "devDependencies": {
    "@dusty-phillips/rescript-zora": "^5.0.1",
    "@microsoft/api-extractor": "^7.47.0",
    "@popstarfreas/rescript-nodejs": "git+https://github.com/popstarfreas/rescript-nodejs.git#f3bd284a3ef17a3fc7cc81ccb7c362b278ea66c7",
    "@types/node": "^20.5.9",
    "esbuild": "^0.25.6",
    "pta": "^1.2.0",
    "typescript": "^5.2.2"
  }
}
